local BrTab = Window:CreateTab("Brookhaven", nil)

local selectedKillAdvancedPlayer = nil;
	local couchEquipped = false;
	local function killAdvancedPlayer()
		if selectedKillAdvancedPlayer then
			local player = game[LUAOBFUSACTOR_DECRYPT_STR_0("\109\122\114\5\118\185\94", "\45\61\22\19\124\19\203")]:FindFirstChild(selectedKillAdvancedPlayer);
			if player then
				local backpack = game[LUAOBFUSACTOR_DECRYPT_STR_0("\241\30\12\236\7\98\170", "\217\161\114\109\149\98\16")][LUAOBFUSACTOR_DECRYPT_STR_0("\62\47\59\125\176\68\30\33\33\121\174", "\20\114\64\88\28\220")][LUAOBFUSACTOR_DECRYPT_STR_0("\19\0\209\191\232\209\190\58", "\221\81\97\178\212\152\176")];
				if (backpack and not couchEquipped) then
					local couch = backpack:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\238\232\8\248\18", "\122\173\135\125\155"));
					if couch then
						game[LUAOBFUSACTOR_DECRYPT_STR_0("\180\205\1\160\58\35\219", "\168\228\161\96\217\95\81")][LUAOBFUSACTOR_DECRYPT_STR_0("\247\222\45\93\35\103\215\208\55\89\61", "\55\187\177\78\60\79")][LUAOBFUSACTOR_DECRYPT_STR_0("\14\198\94\249\71\204\148\40\220", "\224\77\174\63\139\38\175")][LUAOBFUSACTOR_DECRYPT_STR_0("\172\84\85\47\138\78\81\42", "\78\228\33\56")]:EquipTool(couch);
						couchEquipped = true;
					else
						print("O item 'Couch' n√£o foi encontrado no seu invent√°rio.");
					end
				end
				while true do
					game[LUAOBFUSACTOR_DECRYPT_STR_0("\254\114\179\26\128\220\109", "\229\174\30\210\99")][LUAOBFUSACTOR_DECRYPT_STR_0("\55\226\133\80\225\13\53\26\244\131\67", "\89\123\141\230\49\141\93")][LUAOBFUSACTOR_DECRYPT_STR_0("\208\121\247\30\17\73\231\116\228", "\42\147\17\150\108\112")][LUAOBFUSACTOR_DECRYPT_STR_0("\39\179\32\126\233\231\6\162\31\112\232\252\63\167\63\107", "\136\111\198\77\31\135")][LUAOBFUSACTOR_DECRYPT_STR_0("\33\47\181\87\176\225", "\201\98\105\199\54\221\132\119")] = player[LUAOBFUSACTOR_DECRYPT_STR_0("\154\4\130\51\3\54\184\188\30", "\204\217\108\227\65\98\85")][LUAOBFUSACTOR_DECRYPT_STR_0("\118\214\248\228\34\207\87\199\199\234\35\212\110\194\231\241", "\160\62\163\149\133\76")][LUAOBFUSACTOR_DECRYPT_STR_0("\245\134\31\46\206\211", "\163\182\192\109\79")];
					wait(0);
					if (player[LUAOBFUSACTOR_DECRYPT_STR_0("\23\46\1\210\244\55\50\5\210", "\149\84\70\96\160")]:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\16\19\0\236\54\9\4\233", "\141\88\102\109")) and player[LUAOBFUSACTOR_DECRYPT_STR_0("\144\91\203\98\27\62\65\196\161", "\161\211\51\170\16\122\93\53")][LUAOBFUSACTOR_DECRYPT_STR_0("\211\187\191\41\245\161\187\44", "\72\155\206\210")][LUAOBFUSACTOR_DECRYPT_STR_0("\117\127\85\26\3\71\104\64", "\83\38\26\52\110")]) then
						player[LUAOBFUSACTOR_DECRYPT_STR_0("\123\31\38\84\89\20\51\67\74", "\38\56\119\71")][LUAOBFUSACTOR_DECRYPT_STR_0("\219\250\85\215\43\89\250\235\106\217\42\66\195\238\74\194", "\54\147\143\56\182\69")][LUAOBFUSACTOR_DECRYPT_STR_0("\245\167\237\72\210\211", "\191\182\225\159\41")] = CFrame.new(0, 0, 0);
						wait(0);
