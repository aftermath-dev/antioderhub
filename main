						if couch then
							couch[LUAOBFUSACTOR_DECRYPT_STR_0("\42\180\154\8\20\161", "\109\122\213\232")] = nil;
							couchEquipped = false;
						end
					end
				end
			else
				print("Jogador n√£o encontrado.");
			end
		else
			print("Nenhum jogador selecionado para o Bring Avan√ßado.");
		end
	end
	local killAdvancedPlayerList = {};
	for _, player in ipairs(game[LUAOBFUSACTOR_DECRYPT_STR_0("\222\251\163\41\235\229\177", "\80\142\151\194")]:GetPlayers()) do
		table.insert(killAdvancedPlayerList, player.Name);
	end
local Dropdown = BrTab:CreateDropdown({
   Name = "Select player for bring",
